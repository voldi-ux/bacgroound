{
  "name": "natours",
  "version": "1.0.0",
  "description": "",
  "homepage": "https://voldi-ux.github.io/natours",
  "main": "index.js",
  "scripts": {
    "watch_css": "node-sass sass/style.scss css/style.css -w",
    "Dervsever": " live-server ",
    "start": "npm-run-all --parallel Dervsever watch_css",
    "compile_css": "node-sass sass/style.scss css/style.compile.css ",
    "concat_css": "concat -o css/style.concat.css css/icon-font.min.css css/style.compile.css",
    "prefix": "postcss --use autoprefixer -b'last 5' css/style.concat.css -o css/style.prefixed.css ",
    "compressed_css": "node-sass css/style.prefixed.css  css/style.css --output-style compressed",
    "build": "npm-run-all compile_css concat_css prefix compressed_css",
    "predeploy": "npm build",
    "deploy": "gh-pages -d build"
  },
  "author": "voldi",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^9.8.5",
    "concat": "^1.0.3",
    "gh-pages": "^3.1.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1"
  }
}
